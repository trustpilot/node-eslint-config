{
  "name": "eslint-config-trustpilot",
  "version": "4.3.0",
  "description": "Shared ESLint config for Trustpilot projects",
  "homepage": "https://github.com/trustpilot/node-eslint-config",
  "repository": "https://github.com/trustpilot/node-eslint-config.git",
  "author": "Trustpilot",
  "license": "MIT",
  "main": "default.js",
  "scripts": {
    "lint": "eslint --ext .js .",
    "lint:fix": "npm run lint -- --fix"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "eslint": "5.x",
    "eslint-plugin-babel": "5.x",
    "eslint-plugin-html": "5.x",
    "eslint-plugin-prettier": "3.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "prettier": "1.x"
  },
  "devDependencies": {
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/commit-analyzer": "6.3.3",
    "@semantic-release/git": "7.0.18",
    "@semantic-release/github": "5.5.5",
    "@semantic-release/npm": "5.3.4",
    "@semantic-release/release-notes-generator": "7.3.5",
    "babel-eslint": "10.0.3",
    "commitlint": "8.2.0",
    "conventional-changelog-eslint": "3.0.4",
    "eslint": "6.7.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.1.0",
    "prettier": "1.19.1",
    "semantic-release": "15.13.31"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  }
}
